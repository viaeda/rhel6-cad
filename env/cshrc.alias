#************************************************************
# User specific aliases and functions
#************************************************************

alias	startvnc	'vncserver -depth 24 -geometry 1920x1080'
alias	killvnc		'vncserver -kill'

alias	passwd		'yppasswd'
alias	c		'clear'
alias	rm		'rm -i'
alias	del		'rm -rf'
alias	cp		'cp -i'
alias	mv		'mv -i'
alias	ll		'ls -altr'
alias	la		'ls -a'
alias	lx		'ls -x'
alias	pwd		'echo $cwd'
